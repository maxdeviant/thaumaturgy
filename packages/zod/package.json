{
  "name": "@thaumaturgy/zod",
  "version": "0.10.0",
  "repository": "git@github.com:maxdeviant/thaumaturgy.git",
  "author": "Marshall Bowers <elliott.codes@gmail.com>",
  "license": "MIT",
  "description": "Thaumaturgy is a fixtures and seeding library for TypeScript.",
  "keywords": [
    "thaumaturgy",
    "typescript",
    "zod",
    "test",
    "testing",
    "fixture",
    "factory",
    "seed",
    "seeding"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write '**/*.+(js|json|md|ts|tsx)'",
    "build": "tsc -p .",
    "test": "jest",
    "doc": "tsuka './src/**/*.ts'"
  },
  "files": [
    "lib/",
    "!lib/**/__tests__/",
    "!lib/**/*.test.{js,d.ts}"
  ],
  "peerDependencies": {
    "zod": "3.x"
  },
  "dependencies": {
    "@thaumaturgy/core": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "28.1.6",
    "@types/uuid": "8.3.4",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "sqlite": "4.1.2",
    "sqlite3": "5.0.10",
    "ts-jest": "28.0.7",
    "tsuka": "0.0.4",
    "typescript": "4.7.4",
    "zod": "3.19.1"
  }
}
