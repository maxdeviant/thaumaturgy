{
  "name": "thaumaturge-io-ts",
  "version": "0.10.0",
  "repository": "git@github.com:maxdeviant/thaumaturge.git",
  "author": "Marshall Bowers <elliott.codes@gmail.com>",
  "license": "MIT",
  "description": "Thaumaturge is a fixtures and seeding library powered by `io-ts`.",
  "keywords": [
    "thaumaturge",
    "typescript",
    "io-ts",
    "test",
    "testing",
    "fixture",
    "factory",
    "seed",
    "seeding"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write '**/*.+(js|json|md|ts|tsx)'",
    "build": "tsc -p .",
    "test": "jest",
    "doc": "tsuka './src/**/*.ts'"
  },
  "files": [
    "lib/",
    "!lib/**/__tests__/",
    "!lib/**/*.test.{js,d.ts}"
  ],
  "peerDependencies": {
    "fp-ts": "2.x",
    "io-ts": "2.x"
  },
  "dependencies": {
    "thaumaturge": "workspace:*",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/jest": "28.1.6",
    "@types/uuid": "8.3.4",
    "fp-ts": "2.12.2",
    "io-ts": "2.2.17",
    "io-ts-types": "0.5.16",
    "jest": "28.1.3",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "prettier": "2.7.1",
    "sqlite": "4.1.2",
    "sqlite3": "5.0.10",
    "ts-jest": "28.0.7",
    "tsuka": "0.0.4",
    "typescript": "4.7.4"
  }
}
