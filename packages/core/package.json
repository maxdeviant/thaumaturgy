{
  "name": "@thaumaturgy/core",
  "version": "0.14.0",
  "repository": "git@github.com:maxdeviant/thaumaturgy.git",
  "author": "Marshall Bowers <elliott.codes@gmail.com>",
  "license": "MIT",
  "description": "Thaumaturgy is a fixtures and seeding library for TypeScript.",
  "keywords": [
    "thaumaturgy",
    "typescript",
    "test",
    "testing",
    "fixture",
    "factory",
    "seed",
    "seeding"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write '**/*.+(js|json|md|ts|tsx)'",
    "build": "tsc -p .",
    "test": "vitest run",
    "doc": "tsuka './src/**/*.ts'"
  },
  "files": [
    "lib/",
    "!lib/**/__tests__/",
    "!lib/**/*.test.{js,d.ts}"
  ],
  "devDependencies": {
    "@types/node": "18.15.8",
    "prettier": "2.7.1",
    "tsuka": "0.0.4",
    "typescript": "4.7.4",
    "vitest": "0.31.0"
  }
}
