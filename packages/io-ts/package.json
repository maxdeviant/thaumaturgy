{
  "name": "@thaumaturgy/io-ts",
  "version": "0.14.0",
  "repository": "git@github.com:maxdeviant/thaumaturgy.git",
  "author": "Marshall Bowers <elliott.codes@gmail.com>",
  "license": "MIT",
  "description": "Thaumaturgy is a fixtures and seeding library for TypeScript.",
  "keywords": [
    "thaumaturgy",
    "typescript",
    "io-ts",
    "test",
    "testing",
    "fixture",
    "factory",
    "seed",
    "seeding"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write '**/*.+(js|json|md|ts|tsx)'",
    "build": "tsc -p .",
    "test": "vitest run",
    "doc": "tsuka './src/**/*.ts'"
  },
  "files": [
    "lib/",
    "!lib/**/__tests__/",
    "!lib/**/*.test.{js,d.ts}"
  ],
  "peerDependencies": {
    "fp-ts": "2.x",
    "io-ts": "2.x"
  },
  "dependencies": {
    "@thaumaturgy/core": "workspace:*"
  },
  "devDependencies": {
    "@types/better-sqlite3": "7.6.4",
    "better-sqlite3": "8.3.0",
    "fp-ts": "2.12.2",
    "io-ts": "2.2.17",
    "io-ts-types": "0.5.16",
    "kysely": "0.24.2",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "prettier": "2.7.1",
    "tsuka": "0.0.4",
    "typescript": "4.7.4",
    "vitest": "0.31.0"
  }
}
